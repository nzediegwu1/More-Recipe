{
  "dependencies": {
    "body-parser": "~1.8.1",
    "chai": "^4.1.2",
    "express": "~4.9.0",
    "mocha": "^3.5.3",
    "morgan": "~1.3.0"
  },
  "description": "Backend for the more-recipes web app",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.7.2",
    "eslint-config-defaults": "^9.0.0",
    "istanbul": "^0.4.5",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2"
  },
  "main": "bin/www.js",
  "name": "more-recipes",
  "private": true,
  "scripts": {
    "test": "mocha ./tests/* --compilers js:babel-register",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/bin/www.js",
    "coverage": "node node_modules/.bin/istanbul cover_mocha--R spec",
    "coveralls": "cat ./coverge/Icov.info|node node_modules/.bin/coveralls"
  },
  "version": "0.0.0"
}