{
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.8.1",
    "chai": "^4.1.2",
    "dotenv": "^4.0.0",
    "express": "~4.9.0",
    "jsonwebtoken": "^8.0.1",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "~1.3.0",
    "nyc": "^11.2.1",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.13.1"
  },
  "description": "Backend for the more-recipes web app",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.7.2",
    "eslint-config-defaults": "^9.0.0",
    "istanbul": "^0.4.5",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2"
  },
  "main": "bin/www.js",
  "name": "more-recipes",
  "private": true,
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha ./server/tests/ --compilers js:babel-register",
    "start": "nodemon ./server/dist/bin/www.js",
    "build": "babel ./server/ -d ./server/dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "postinstall": "npm run build",
    "start:dev": "nodemon --watch server/ --exec babel-node  -- server/bin/www.js"
